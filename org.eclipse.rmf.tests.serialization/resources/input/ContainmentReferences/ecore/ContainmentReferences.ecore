        <details
            key="name"
            value="CHILD"/>
        <details
            key="kind"
            value="element"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers
      xsi:type="ecore:EClass"
      name="Node">
    <eAnnotations
        source="http:///org/eclipse/emf/ecore/util/ExtendedMetaData">
      <details
